    <text>财务管理</text>
  </view>
  <view class='kuai'>
    <view style='text-align: right;padding-right: 10rpx;'>今日收益</view>
    <view style="padding:0 0 30rpx 20rpx; font-weight:700; color: #FF6702;">7890</view>
  </view>
  <view class='kuai'>
    <view style='text-align: right;padding-right: 10rpx;'>总计收益</view>
    <view style="padding:0 0 30rpx 20rpx; font-weight:700; color: #FF6702;">6250</view>
  </view>
    <view class='kuai'>
