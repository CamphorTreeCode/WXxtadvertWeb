    <view style="padding:0 0 30rpx 20rpx; font-weight:700; color: #FF6702;">3390</view>
  </view>
  <view class='kuai'>
    <view style='text-align: right;padding-right: 10rpx;'>余额</view>
    <view style="padding:0 0 30rpx 20rpx; font-weight:700; color: #FF6702;">3390</view>
  </view>
  <view class='kuai money'>
