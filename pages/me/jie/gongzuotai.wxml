  <text style='float:left; margin-left: 20rpx;'>设置支付密码</text>
  <text style='float:right; margin-right: 20rpx;'>></text>
</view>
<!-- 退出登录弹框 -->
<view class='shadow {{state==true?"kuang":" "}}'></view>
<view class='tan {{state==true?"kuang":" "}}'>
